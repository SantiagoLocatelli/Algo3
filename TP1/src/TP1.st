Object subclass: #AlgoFix	instanceVariableNames: 'pintores'	classVariableNames: ''	package: 'TP1'!!AlgoFix methodsFor: 'initialization' stamp: 'SantiagoLocatelli 5/17/2020 15:33'!initialize	pintores := OrderedCollection new.! !!AlgoFix methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/17/2020 18:33'!manoDeObra: precioObra masMateriales: precioMateriales 		^precioObra + precioMateriales.! !!AlgoFix methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/17/2020 17:46'!crearPintura: nombre conPrecioPorLitro: precio manosPincel: manosP manosRodillo: manosR		| pintura |	pintura := Pintura crearConNombre: nombre conPrecioPorLitro: precio manosDePincel: manosP manosDeRodillo: manosR.	^pintura.! !!AlgoFix methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/24/2020 22:38'!registrarPintorDeRodillo: unNombre conValorHora: valorPorHora 	| pintor |	pintor := PintorDeRodillo conNombre: unNombre yConValorHora: valorPorHora.	pintores add: pintor.! !!AlgoFix methodsFor: 'access' stamp: 'SantiagoLocatelli 5/27/2020 23:54'!registrarPintorDePincel: unNombre conValorHora: unValor	| pintor |	pintor := PintorDePincel conNombre: unNombre yConValorHora: unValor.	pintores add: pintor.	! !!AlgoFix methodsFor: 'access' stamp: 'SantiagoLocatelli 5/26/2020 22:41'!presupuestoMasBaratoParaPintarMetrosCuadrados: cantidadMetros conPintura: unaPintura	| pintorMasBarato presupuesto |	pintores isEmpty ifTrue: [ NoTieneElementosError signal ].	pintorMasBarato := pintores detectMin: [ :unPintor | unPintor calcularPresupuestoDe: unaPintura deMetrosCuadrados: cantidadMetros ].	presupuesto := pintorMasBarato crearPresupuestoDe: unaPintura deMetrosCuadrados: cantidadMetros.	^presupuesto.! !Error subclass: #NoTieneElementosError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Error subclass: #NumeroInvalidoError	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!Object subclass: #Pintor	instanceVariableNames: 'nombre valorPorHora horasPorM2 litrosPorM2'	classVariableNames: ''	package: 'TP1'!!Pintor methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/19/2020 01:37'!getValorPorHora 	^valorPorHora.! !!Pintor methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/26/2020 23:59'!calcularPresupuestoDe: unaPintura deMetrosCuadrados: cantidadMetros	| valorPresupuesto |	valorPresupuesto :=  (self calcularCostoMaterialesDe: unaPintura deMetros: cantidadMetros) + (self calcularManoDeObraDe:  unaPintura deMetrosCuadrados: cantidadMetros).	^valorPresupuesto.! !!Pintor methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/26/2020 22:41'!crearPresupuestoDe: unaPintura deMetrosCuadrados: cantidadMetros	| presupuesto |	presupuesto := Presupuesto conResponsable: nombre deValor: (self calcularPresupuestoDe: unaPintura deMetrosCuadrados: cantidadMetros).	^presupuesto.! !!Pintor methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/26/2020 23:59'!calcularManoDeObraDe: unaPintura deMetrosCuadrados: cantidadMetros		^self subclassResponsibility.! !!Pintor methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/24/2020 22:27'!getNombre	^nombre.! !!Pintor methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/27/2020 22:19'!calcularCostoMaterialesDe: unaPintura deMetros: cantidadMetros	^self subclassResponsibility.! !!Pintor methodsFor: 'initialization' stamp: 'SantiagoLocatelli 5/17/2020 16:39'!setNombre: unNombre	nombre := unNombre.! !!Pintor methodsFor: 'initialization' stamp: 'SantiagoLocatelli 5/17/2020 22:29'!setValorPorHora: unValor.	valorPorHora := unValor.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintor class	instanceVariableNames: ''!!Pintor class methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/24/2020 22:38'!conNombre: unNombre yConValorHora: unValor		^self subclassResponsibility.! !Pintor subclass: #PintorDePincel	instanceVariableNames: ''	classVariableNames: 'descuento'	package: 'TP1'!!PintorDePincel methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/26/2020 22:41'!crearPresupuestoDe: unaPintura deMetrosCuadrados: cantidadMetros	^super crearPresupuestoDe: unaPintura deMetrosCuadrados: cantidadMetros.! !!PintorDePincel methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/21/2020 17:06'!calcularPresupuestoDe: unaPintura deMetrosCuadrados: cantidadMetros	^super calcularPresupuestoDe: unaPintura deMetrosCuadrados: cantidadMetros.! !!PintorDePincel methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/28/2020 09:59'!calcularManoDeObraDe: unaPintura deMetrosCuadrados: cantidadMetros		(cantidadMetros > 40) ifTrue: [		^unaPintura calcularManoDeObraConPincel: (horasPorM2 * cantidadMetros * valorPorHora * descuento).		] ifFalse: [ 			^unaPintura calcularManoDeObraConPincel: (horasPorM2 * cantidadMetros * valorPorHora).		].! !!PintorDePincel methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/26/2020 23:53'!getDescuento	^descuento.! !!PintorDePincel methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/27/2020 22:26'!calcularCostoMaterialesDe: unaPintura deMetros: cantidadMetros	^unaPintura calcularCostoMaterialesConPincel: cantidadMetros conLitrosPorMetros: litrosPorM2.! !!PintorDePincel methodsFor: 'initialization' stamp: 'SantiagoLocatelli 5/24/2020 22:40'!initialize		descuento := 0.5.	horasPorM2 := 2.	litrosPorM2 := 4.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PintorDePincel class	instanceVariableNames: ''!!PintorDePincel class methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/24/2020 22:38'!conNombre: unNombre yConValorHora: unValor		| pintor |	(unValor < 0) ifTrue: [ NumeroInvalidoError signal ].	pintor := self new. 	pintor setNombre: unNombre.	pintor setValorPorHora: unValor.	^pintor.! !Pintor subclass: #PintorDeRodillo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!PintorDeRodillo methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/26/2020 22:41'!crearPresupuestoDe: unaPintura deMetrosCuadrados: cantidadMetros	^super crearPresupuestoDe: unaPintura deMetrosCuadrados: cantidadMetros.! !!PintorDeRodillo methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/21/2020 17:06'!calcularPresupuestoDe: unaPintura deMetrosCuadrados: cantidadMetros	^super calcularPresupuestoDe: unaPintura deMetrosCuadrados: cantidadMetros.! !!PintorDeRodillo methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/28/2020 10:02'!calcularManoDeObraDe: unaPintura deMetrosCuadrados: cantidadMetros		^unaPintura calcularManoDeObraConRodillo: (cantidadMetros * valorPorHora * horasPorM2).! !!PintorDeRodillo methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/27/2020 22:19'!calcularCostoMaterialesDe: unaPintura deMetros: cantidadMetros	^unaPintura calcularCostoMaterialesConRodillo: cantidadMetros conLitrosPorMetros: litrosPorM2.! !!PintorDeRodillo methodsFor: 'initialization' stamp: 'SantiagoLocatelli 5/24/2020 22:40'!initialize 	horasPorM2 := 1.	litrosPorM2 := 5.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PintorDeRodillo class	instanceVariableNames: ''!!PintorDeRodillo class methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/24/2020 22:39'!conNombre: unNombre yConValorHora: unValor		| pintor |	(unValor < 0)ifTrue: [ NumeroInvalidoError signal ].	pintor := self new. 	pintor setNombre: unNombre.	pintor setValorPorHora: unValor.	^pintor.! !Object subclass: #Pintura	instanceVariableNames: 'precioPorLitro manosPincel manosRodillo nombre'	classVariableNames: ''	package: 'TP1'!!Pintura methodsFor: 'initialization' stamp: 'SantiagoLocatelli 5/17/2020 16:22'!setManosRodillo: manosR	manosRodillo := manosR.! !!Pintura methodsFor: 'initialization' stamp: 'SantiagoLocatelli 5/17/2020 16:21'!setPrecioPorLitro: unPrecio		precioPorLitro := unPrecio.! !!Pintura methodsFor: 'initialization' stamp: 'SantiagoLocatelli 5/17/2020 16:20'!setNombre: unNombre.	nombre := unNombre.! !!Pintura methodsFor: 'initialization' stamp: 'SantiagoLocatelli 5/17/2020 16:22'!setManosPincel: manosP	manosPincel := manosP.! !!Pintura methodsFor: 'accesing' stamp: 'SantiagoLocatelli 5/19/2020 09:43'!getManosPincel	^manosPincel.! !!Pintura methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/22/2020 01:14'!getPrecioPorLitro	^precioPorLitro.! !!Pintura methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/28/2020 09:57'!calcularManoDeObraConPincel: costoPintor 	^manosPincel * costoPintor.! !!Pintura methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/19/2020 09:43'!getManosRodillo	^manosRodillo.! !!Pintura methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/19/2020 09:37'!calcularCostoMaterialesConRodillo: cantidadMetros conLitrosPorMetros: litrosPorMetros	^cantidadMetros * litrosPorMetros * precioPorLitro * manosRodillo.! !!Pintura methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/22/2020 01:03'!getNombre	^nombre.! !!Pintura methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/19/2020 09:35'!calcularCostoMaterialesConPincel: cantidadMetros conLitrosPorMetros: litrosPorMetros	^cantidadMetros * litrosPorMetros * precioPorLitro * manosPincel.! !!Pintura methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/28/2020 10:00'!calcularManoDeObraConRodillo: costoPintor 	^manosRodillo * costoPintor.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pintura class	instanceVariableNames: ''!!Pintura class methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/24/2020 18:43'!crearConNombre: unNombre conPrecioPorLitro: precio manosDePincel: manosP manosDeRodillo: manosR	| pintura |		(manosP < 0) | (manosR < 0) | (precio < 0) ifTrue: [ NumeroInvalidoError signal].		pintura := self new.	pintura setNombre: unNombre.	pintura setPrecioPorLitro: precio.	pintura setManosPincel: manosP.	pintura setManosRodillo: manosR.	^pintura.! !Object subclass: #Presupuesto	instanceVariableNames: 'valor responsable'	classVariableNames: ''	package: 'TP1'!!Presupuesto methodsFor: 'initialization' stamp: 'SantiagoLocatelli 5/17/2020 22:20'!setValor: unValor	valor := unValor.! !!Presupuesto methodsFor: 'accessing' stamp: 'SantiagoLocatelli 5/17/2020 22:19'!setResponsable: unNombre	responsable := unNombre.! !!Presupuesto methodsFor: 'accessing' stamp: 'SantiagoLocatelli 5/17/2020 15:25'!valor	^valor.! !!Presupuesto methodsFor: 'accessing' stamp: 'SantiagoLocatelli 5/17/2020 15:26'!responsable	^responsable.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Presupuesto class	instanceVariableNames: ''!!Presupuesto class methodsFor: 'as yet unclassified' stamp: 'SantiagoLocatelli 5/26/2020 00:38'!conResponsable: unNombre deValor: unValor	| presupuesto |	( unValor < 0 ) ifTrue: [ NumeroInvalidoError signal ].	presupuesto := self new.	presupuesto setResponsable: unNombre.	presupuesto setValor: unValor.	^presupuesto. ! !