Object subclass: #ComisionCurricularPermanente	instanceVariableNames: 'expedientes expedienteAProcesar'	classVariableNames: ''	package: 'tp0'!!ComisionCurricularPermanente methodsFor: 'accesing' stamp: 'Santi 4/17/2020 20:24'!procesarExpediente| expedienteActual |	expedienteActual := ( expedientes at: expedienteAProcesar ).	expedienteActual procesar.	expedienteAProcesar := expedienteAProcesar + 1.	^expedienteActual.! !!ComisionCurricularPermanente methodsFor: 'accesing' stamp: 'Santi 4/17/2020 19:55'!votarAFavor	(expedientes at: expedienteAProcesar) sumarVotoAFavor.	! !!ComisionCurricularPermanente methodsFor: 'accesing' stamp: 'Santi 4/17/2020 19:56'!votarEnContra 	( expedientes at: expedienteAProcesar ) sumarVotoEnContra.! !!ComisionCurricularPermanente methodsFor: 'accesing' stamp: 'Santi 4/17/2020 19:48'!recibirExpediente: expedienteNuevo	expedientes add: expedienteNuevo.! !!ComisionCurricularPermanente methodsFor: 'initialization' stamp: 'Santi 4/17/2020 19:47'!initialize	expedientes := OrderedCollection new.	expedienteAProcesar := 1.! !Object subclass: #Expediente	instanceVariableNames: 'nombre estado votosAFavor votosEnContra'	classVariableNames: ''	package: 'tp0'!!Expediente methodsFor: 'accessing' stamp: 'Santi 4/17/2020 20:37'!estado	^estado.! !!Expediente methodsFor: 'accessing' stamp: 'Santi 4/17/2020 20:37'!votosAFavor	^votosAFavor.! !!Expediente methodsFor: 'accessing' stamp: 'Santi 4/17/2020 21:23'!procesar	estado := votosAFavor < votosEnContra		ifTrue: [ 'Rechazado' ]		ifFalse: [ 'Aprobado' ]! !!Expediente methodsFor: 'accessing' stamp: 'Santi 4/17/2020 20:38'!votosEnContra	^votosEnContra.! !!Expediente methodsFor: 'initialization' stamp: 'Santi 4/17/2020 21:30'!initialize	nombre := ' '.	estado := 'Iniciado'.	votosEnContra := 0.	votosAFavor := 0.! !!Expediente methodsFor: 'accesing' stamp: 'Santi 4/17/2020 19:55'!sumarVotoAFavor	votosAFavor := votosAFavor + 1.! !!Expediente methodsFor: 'accesing' stamp: 'Santi 4/17/2020 20:24'!establecerNombre: nuevoNombre	nombre := nuevoNombre.! !!Expediente methodsFor: 'accesing' stamp: 'Santi 4/17/2020 19:57'!sumarVotoEnContra	votosEnContra := votosEnContra + 1.! !